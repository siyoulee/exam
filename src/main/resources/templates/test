    <div class="answer answer_for_




          " id="answer_50446" style="" title="Only exists at the level of the physical data model.">


      <span class="hidden id">50446</span>

    <div class="select_answer answer_type">
        <input id="answer-50446" name="question-71868" type="radio" class="question_input" aria-disabled="true" disabled="">&nbsp;
      <label for="answer-50446">
        <div class="answer_text">Only exists at the level of the physical data model</div>
        <div class="answer_html"></div>
      </label>
    </div>
    <div class="answer_type short_answer" style="display:none;">
      <input name="answer_text" type="text" style="width: 484.5px; margin-bottom: 5px; color: rgb(0, 0, 0);" disabled="true" value="Only exists at the level of the physical data model">
    </div>
    <div class="answer_match matching_answer answer_type" style="display:none;">
